{
    "manifest_version": 3,
    "name": "To Do side panel",
    "version": "1.0",
    "description": "Shows how to display the same side panel on every site using the Side Panel API.",
    "icons": {
      "16": "Images/Mouse.png",
      "48": "Images/Mouse.png",
      "128": "Images/Mouse.png"
    },
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "permissions": ["sidePanel"]
  }